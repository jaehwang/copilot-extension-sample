{
  "name": "jira-github-copilot-chat-extension",
  "version": "1.0.0",
  "description": "A GitHub Copilot Chat extension for integrating with Jira.",
  "main": "dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "keywords": [
    "github",
    "copilot",
    "jira",
    "extension"
  ],
  "author": "Jaehwang Kim",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "@vscode/chat-extension-utils": "^0.0.0-alpha.1",
		"@vscode/prompt-tsx": "^0.3.0-alpha.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.95.0",
		"eslint": "^9.13.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
 
  "contributes": {
    "commands": [
      {
        "command": "createIssue",
        "title": "Create Jira Issue"
      },
      {
        "command": "fetchIssues",
        "title": "Fetch Jira Issues"
      }
    ]
  }
}
